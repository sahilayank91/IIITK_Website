<!--<div ng-app="IIITKWebsite" ng-controller="BatchController">-->
    <div class="mdl-grid demo-content">
        <div class="demo-charts mdl-color--white mdl-shadow--2dp mdl-cell mdl-cell--12-col mdl-grid">
            <form method="POST" name="batch" style="width:inherit">
                <!--<div class="container">-->
                    <h1>Add new Batch</h1>
                    <hr>
                    <label><b>Year</b></label>
                    <!--<input type="text" placeholder="Year" name="year" ng-model="year">-->
                    <select type="text" ng-model="year" ng-options="x for x in year_options" class="select_bar"></select>
                    <br><br>

                    <label><b>Type</b></label>
                    <!--<input type="text" placeholder="Type" name="type" ng-model="type">-->
                    <select type="text" ng-model="type" ng-options="item for item in type_options" class="select_bar"></select>
                    <br><br>

                    <label><b>Branch</b></label>
                    <!--<input type="text" placeholder="Branch" name="branch" ng-model="branch">-->
                    <select type="text" ng-model="branch" ng-options="item for item in branch_options" class="select_bar"></select>
                    <br><br>

                    <div class="row">
                        <div class="col-md-5">
                            <label><b>Student Name</b></label>
                        </div>
                        <div class="col-md-1">
                        </div>

                        <div class="col-md-5">
                            <label><b>Student ID</b></label>
                        </div>
                        <div class="col-md-1">
                        </div>
                    </div>
                    <div class="row" ng-repeat="student in students">
                        <div class="col-md-5">
                            <input type="text" placeholder="Student Name" name="name" ng-model="student.name">
                        </div>
                        <div class="col-md-1">
                        </div>

                        <div class="col-md-5">
                            <input type="text" placeholder="Student Id" name="id" ng-model="student.college_id">
                        </div>
                        <div class="col-md-1">
                            <img src="/images/trashbtn.png" class="trashbtn" ng-click="removeStudentRow($index)">
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-5">
                            <input type="text" placeholder="Student Name" name="name" ng-model="s_name">
                        </div>
                        <div class="col-md-1">
                        </div>
                        <div class="col-md-5">
                            <input type="text" placeholder="Student Id" name="id" ng-model="s_id">
                        </div>
                        <div class="col-md-1">
                            <img src="/images/plusbtn.png" class="addbtn" ng-click="addStudentRow()">
                        </div>
                    </div>


                <!--<div class="clearfix">-->
                        <button type="submit" class="cancelbtn" ng-click="goToAllBatches()">CANCEL</button>
                        <button type="submit" class="signupbtn" ng-click="addNewBatch()">ADD</button>
                    <!--</div>-->
                <!--</div>-->
            </form>
        </div>
    </div>
<!--</div>-->